SELECT CUSTOMER_TYPE_DESCRIPTION, LINE_OFFICER_RISK_RATING_CODE, CREDIT_REVIEW_RISK_RATING_CODE, REGULATOR_RISK_RATING_CODE, COUNT(*) AS RISK_COUNT FROM CUSTOMER_DIMENSION GROUP BY CUSTOMER_TYPE_DESCRIPTION, LINE_OFFICER_RISK_RATING_CODE, CREDIT_REVIEW_RISK_RATING_CODE, REGULATOR_RISK_RATING_CODE ORDER BY RISK_COUNT DESC LIMIT 100